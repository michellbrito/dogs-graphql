<h1 class="display-1 text-center"> 🦴 Dogs 🐕</h1>

<div class="container text-center">
  <div class="row">
    {{#each data}}
      <div class="col-sm-12 col-md-6 col-lg-4 img">
        <img src={{this.img}} />
        <div class="overlay">
          <div class="text">
            <p>{{this.breed}}</p>
            </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>